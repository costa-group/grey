{
    "language": "Solidity",
    "sources": {
        "for_loop_break.sol": {
            "content": "contract C {\n    function f() public returns (uint i) {\n        assembly {\n            for {} lt(i, 10) { i := add(i, 1) }\n            {\n                if eq(i, 6) { break }\n                i := add(i, 1)\n            }\n        }\n    }\n}\n// ----\n// f() -> 6\n"
        }
    },
